{
    "name": "dragon-code/laravel-routes-core",
    "description": "The core of applications for working with routes",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "routes",
        "pretty"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/laravel-routes-core/issues",
        "source": "https://github.com/TheDragonCode/laravel-routes-core"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "dragon-code/contracts": "^2.4",
        "dragon-code/laravel-support": "^3.0",
        "dragon-code/support": "^5.0",
        "illuminate/routing": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "phpdocumentor/reflection-docblock": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "conflict": {
        "andrey-helldar/laravel-routes-core": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\LaravelRoutesCore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
